{
  "env": "development",
  "port": 3000,
  "swagger": {
    "enabled": true,
    "endpoint": "docs"
  },
  "rateLimit": {
    "enabled": true,
    "max": 100,
    "timeWindow": "1m"
  },
  "corsOrigin": "*",
  "corsMethods": "*",
  "corsHeaders": "*",
  "validatorForbidUnknown": true,
  "loggerTimestampFormat": "YYYY-MM-DD HH:mm:ss.SSS"
}
